<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.fta.greendaotest.MainActivity"
    tools:ignore="RtlHardcoded">


    <Button
        android:id="@+id/simple_greendao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/simple_greendao"
        android:onClick="jumpto"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="138dp"
        android:layout_marginTop="75dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="138dp" />

    <Button
        android:id="@+id/rx_greendao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/rx_greendao"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="105dp"
        android:onClick="jumpto"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="66dp"
        app:layout_constraintTop_toBottomOf="@+id/simple_greendao"
        android:layout_marginRight="105dp" />
</android.support.constraint.ConstraintLayout>
